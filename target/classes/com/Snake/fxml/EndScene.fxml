<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="startAnchorPane" maxHeight="560.0" maxWidth="870.0" minHeight="560.0" minWidth="870.0" prefHeight="560.0" prefWidth="870.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Snake.controller.EndSceneController">
   <Pane fx:id="leftsidePane" prefHeight="560.0" prefWidth="552.0">
      <children>
         <Label fx:id="playernameLabel" layoutX="35.0" layoutY="211.0" prefHeight="58.0" prefWidth="482.0" text="PLAYER :" textFill="WHITE">
            <font>
               <Font name="Monospaced Regular" size="32.0" />
            </font>
         </Label>
         <Label fx:id="playerscoreLabel" layoutX="35.0" layoutY="269.0" prefHeight="58.0" prefWidth="482.0" text="SCORE :" textFill="WHITE">
            <font>
               <Font name="Monospaced Regular" size="32.0" />
            </font>
         </Label>
      </children>
   </Pane>
   <Button fx:id="rightButton" layoutX="595.0" layoutY="214.0" mnemonicParsing="false" onAction="#SwitchToStartScene" prefHeight="66.0" prefWidth="275.0" text="Main Menu">
      <font>
         <Font name="Monospaced Regular" size="32.0" />
      </font>
   </Button>
   <Button fx:id="rightButton" layoutX="595.0" layoutY="314.0" mnemonicParsing="false" onAction="#SwitchToLeaderboardScene" prefHeight="66.0" prefWidth="275.0" text="Leaderboard">
      <font>
         <Font name="Monospaced Regular" size="32.0" />
      </font>
   </Button>
   <Label layoutX="346.0" layoutY="43.0" prefHeight="17.0" prefWidth="413.0" text="GAME OVER" textFill="WHITE">
      <font>
         <Font name="Monospaced Bold Italic" size="75.0" />
      </font>
   </Label>
   <Button fx:id="rightButton" layoutX="595.0" layoutY="414.0" mnemonicParsing="false" onAction="#ExitGame" prefHeight="66.0" prefWidth="275.0" text="Exit">
      <font>
         <Font name="Monospaced Regular" size="32.0" />
      </font>
   </Button>
</AnchorPane>
