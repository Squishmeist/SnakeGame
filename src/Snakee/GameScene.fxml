<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="GameScene" onKeyPressed="#keyPressed" prefHeight="560.0" prefWidth="870.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Snakee.GameSceneController">
   <children>
      <Label fx:id="playernameLabel" layoutX="14.0" prefHeight="58.0" prefWidth="186.0" text="Player Name">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="playerscoreLabel" layoutX="14.0" layoutY="58.0" prefHeight="58.0" prefWidth="186.0" text="Player Score">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="14.0" layoutY="521.0" mnemonicParsing="false" onAction="#switchToStartScene" text="Back" />
   </children>
</AnchorPane>
