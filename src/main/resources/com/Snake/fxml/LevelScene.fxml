<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="levelAnchorPane" prefHeight="560.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Snake.controller.LevelSceneController">
   <children>
      <Label fx:id="levelLabel" layoutX="121.0" layoutY="179.0" text="LEVEL :">
         <font>
            <Font name="Monospaced Regular" size="62.0" />
         </font>
      </Label>
      <Label fx:id="scoreLabel" layoutX="121.0" layoutY="280.0" text="SCORE :">
         <font>
            <Font name="Monospaced Regular" size="62.0" />
         </font>
      </Label>
      <Button fx:id="rightButton" layoutX="620.0" layoutY="480.0" mnemonicParsing="false" onAction="#SwitchToGameScene" prefHeight="79.0" prefWidth="250.0" stylesheets="@../style/style.css" text="Next Level">
         <font>
            <Font name="Monospaced Regular" size="30.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
